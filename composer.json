{
    "name": "innmind/object-graph",
    "type": "library",
    "description": "Extract object graph out of a root object",
    "keywords": ["object graph", "graphviz", "dot"],
    "homepage": "http://github.com/Innmind/ObjectGraph",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/ObjectGraph/issues"
    },
    "require": {
        "php": "~8.1",
        "innmind/foundation": "~1.3",
        "innmind/graphviz": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\ObjectGraph\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\ObjectGraph\\": "tests/",
            "Fixtures\\Innmind\\ObjectGraph\\": "fixtures/"
        }
    },
    "require-dev": {
        "innmind/black-box": "^6.4.1",
        "innmind/static-analysis": "^1.2.1",
        "innmind/coding-standard": "~2.0"
    }
}
